<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="nlog" type="NLog.Config.ConfigSectionHandler, NLog" />
    <section name="imageSaving" type="MusicExplorer.Config.ImageSavingConfigurationSection, MusicExplorer" />
    <!--<section name="musicCollection" type="MusicExplorer.Config.MusicCollectionConfigurationSection, MusicExplorer" />-->
  </configSections>
  <imageSaving folder="E:\Проекты\music-explorer\WIMP images" format="[?artist] - [?album] [[?albumId]]" strategy="Rewrite" />
  <!--<musicCollection fileNameFormat="" format="[?artist] - [?album] - [?tracknumber] - [?title]">
    <folders>
      <folder path="E:\Новые песни в плеер" />
      <folder path="\\home-sweet-home\music\Медиатека iPod" />
    </folders>
  </musicCollection>-->
  <nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <extensions>
      <add assembly="MusicExplorer" />
    </extensions>
    <targets>
      <target xsi:type="File" name="logfile" layout="${longdate} ${level}: ${message}" encoding="UTF-8" fileName="MusicExplorer.log" />
    </targets>
    <rules>
      <logger name="*" minLevel="Debug" writeTo="logfile" />
    </rules>
  </nlog>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
  </startup>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="HtmlAgilityPack" publicKeyToken="bd319b19eaf3b43a" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-1.8.9.0" newVersion="1.8.9.0" />
      </dependentAssembly>
    </assemblyBinding>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-11.0.0.0" newVersion="11.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>